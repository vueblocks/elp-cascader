{
  "name": "@vueblocks/elp-cascader",
  "version": "0.2.0",
  "description": "A vue component based on 'element-ui' and 'vue-virtual-scroller'",
  "license": "MIT",
  "main": "lib/elp-cascader.umd.min.js",
  "homepage": "https://vueblocks.github.io/elp-cascader/",
  "repository": "https://github.com/vueblocks/elp-cascader",
  "keywords": [
    "vueblocks",
    "cascader",
    "vue",
    "element",
    "virtual-scroller"
  ],
  "scripts": {
    "serve": "vue-cli-service serve --open",
    "build": "vue-cli-service build --target lib --dest lib --name elp-cascader packages/index.js",
    "lint": "vue-cli-service lint",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook -o ./docs"
  },
  "dependencies": {
    "element-ui": "^2.14.1",
    "throttle-debounce": "^1.0.1",
    "vue-virtual-scroller": "^1.0.10"
  },
  "peerDependencies": {
    "vue": "^2.6.11"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@storybook/addon-actions": "^6.1.11",
    "@storybook/addon-essentials": "^6.1.11",
    "@storybook/addon-links": "^6.1.11",
    "@storybook/vue": "^6.1.11",
    "@vue/babel-preset-app": "^4.5.9",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "babel-plugin-component": "^1.1.1",
    "conventional-changelog-cli": "^3.0.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "less": "^3.12.2",
    "less-loader": "^7.1.0",
    "mockjs": "^1.1.0",
    "vue": "^2.6.12",
    "vue-cli-plugin-element": "^1.0.1",
    "vue-template-compiler": "^2.6.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {
      "no-debugger": "off"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "files": [
    "lib",
    "packages",
    "docs",
    "LICENSE"
  ]
}
